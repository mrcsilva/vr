;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns1.nemvrelas.pt. admin.nemvrelas.pt. (
                  3       ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800 )   ; Negative Cache TTL
;
; name servers - NS records
     IN      NS      ns1.nemvrelas.pt.
     IN      NS      ns2.nemvrelas.pt.

; name servers - A records
ns1.nemvrelas.pt.          IN      A       10.0.0.20
ns2.nemvrelas.pt.          IN      A       10.0.0.21

